<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#5d515e"
    android:id="@+id/container"
    >


    <ToggleButton
        android:id="@+id/active"
        android:layout_width="144dp"
        android:layout_height="144dp"
        android:layout_gravity="right"
        android:background="@drawable/toggle_selector"
        android:textOff=" "
        android:textOn=" "
        app:layout_constraintBottom_toBottomOf="@+id/lottie"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/lottie" />


    <ToggleButton
        android:id="@+id/stop"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_gravity="right"
        android:background="@drawable/outline_stop_circle_24"
        android:textOff=" "
        android:textOn=" "
        app:layout_constraintBottom_toBottomOf="@+id/active"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.914"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/active"
        app:layout_constraintVertical_bias="0.431" />

    <ToggleButton
        android:id="@+id/record"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_gravity="right"
        android:background="@drawable/outline_radio_button_checked_24"
        android:textOff=" "
        android:textOn=" "
        app:layout_constraintBottom_toBottomOf="@+id/active"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.078"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/active"
        app:layout_constraintVertical_bias="0.431" />

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/lottie"
        android:layout_width="529dp"
        android:layout_height="476dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023"
        app:lottie_autoPlay="true"
        app:lottie_loop="true"
        app:lottie_rawRes="@raw/background" />

    <TextView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="250dp"
        android:background="@drawable/square"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Effects"
        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/favorite"
        app:layout_constraintEnd_toEndOf="@+id/effects"
        app:layout_constraintStart_toStartOf="@+id/effects"
        app:layout_constraintTop_toBottomOf="@+id/effects"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sounds"
        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/recordings"
        app:layout_constraintEnd_toEndOf="@+id/sounds"
        app:layout_constraintStart_toStartOf="@+id/sounds"
        app:layout_constraintTop_toBottomOf="@+id/sounds"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="21dp"
        android:layout_marginRight="21dp"
        android:text="Settings"
        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/logOut"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/settings"
        app:layout_constraintTop_toBottomOf="@+id/settings"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sensors"
        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/profile"
        app:layout_constraintEnd_toEndOf="@+id/sensor"
        app:layout_constraintStart_toStartOf="@+id/sensor"
        app:layout_constraintTop_toBottomOf="@+id/sensor"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Favorites"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/favorite"
        app:layout_constraintStart_toStartOf="@+id/favorite"
        app:layout_constraintTop_toBottomOf="@+id/favorite"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="16dp"
        android:text="Log Out"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.937"
        app:layout_constraintStart_toStartOf="@+id/logOut"
        app:layout_constraintTop_toBottomOf="@+id/logOut"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="About"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/recordings"
        app:layout_constraintStart_toStartOf="@+id/recordings"
        app:layout_constraintTop_toBottomOf="@+id/recordings"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Profile"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/profile"
        app:layout_constraintStart_toStartOf="@+id/profile"
        app:layout_constraintTop_toBottomOf="@+id/profile"
        app:layout_constraintVertical_bias="0.0" />


    <TextView
        android:id="@+id/settings"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/round"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.98"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.156" />

    <TextView
        android:id="@+id/settingsImg"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:background="@drawable/round_settings_24"
        android:onClick="onStartSettings"
        app:layout_constraintBottom_toBottomOf="@+id/settings"
        app:layout_constraintEnd_toEndOf="@+id/settings"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/settings"
        app:layout_constraintTop_toTopOf="@+id/settings"
        app:layout_constraintVertical_bias="0.533" />

    <TextView
        android:id="@+id/sensor"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/round"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintHorizontal_bias="0.025"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.156" />

    <TextView
        android:id="@+id/profile"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/round"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintHorizontal_bias="0.025"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.782" />

    <TextView
        android:id="@+id/profileImg"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:onClick="onStartProfile"
        android:background="@drawable/round_perm_identity_24"
        app:layout_constraintBottom_toBottomOf="@+id/profile"
        app:layout_constraintEnd_toEndOf="@+id/profile"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="@+id/profile"
        app:layout_constraintTop_toTopOf="@+id/profile"
        app:layout_constraintVertical_bias="0.533" />



    <TextView
        android:id="@+id/sensorImg"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:background="@drawable/baseline_sensors_24"
        android:onClick="onStartSensors"
        app:layout_constraintBottom_toBottomOf="@+id/sensor"
        app:layout_constraintEnd_toEndOf="@+id/sensor"
        app:layout_constraintStart_toStartOf="@+id/sensor"
        app:layout_constraintTop_toTopOf="@+id/sensor" />

    <TextView
        android:id="@+id/recordings"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/round"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.661"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.782" />

    <TextView
        android:id="@+id/recordingsImg"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:background="@drawable/outline_info_24"
        app:layout_constraintBottom_toBottomOf="@+id/recordings"
        app:layout_constraintEnd_toEndOf="@+id/recordings"
        app:layout_constraintStart_toStartOf="@+id/recordings"
        app:layout_constraintTop_toTopOf="@+id/recordings"
        android:onClick="onStartInfo"/>

    <TextView
        android:id="@+id/sounds"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/round"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.66"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.156" />


    <TextView
        android:id="@+id/soundsImg"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:background="@drawable/round_music_note_24"
        app:layout_constraintBottom_toBottomOf="@+id/sounds"
        app:layout_constraintEnd_toEndOf="@+id/sounds"
        app:layout_constraintStart_toStartOf="@+id/sounds"
        app:layout_constraintTop_toTopOf="@+id/sounds"
        tools:ignore="MissingConstraints"
        android:onClick="onStartSounds"/>

    <TextView
        android:id="@+id/effects"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:background="@drawable/round"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.318"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.156" />

    <TextView
        android:id="@+id/effectsImg"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/round_graphic_eq_24"
        app:layout_constraintBottom_toBottomOf="@+id/effects"
        app:layout_constraintEnd_toEndOf="@+id/effects"
        app:layout_constraintStart_toStartOf="@+id/effects"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        android:onClick="onStartEffects"/>

    <TextView
        android:id="@+id/favorite"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:background="@drawable/round"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.318"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.782" />

    <TextView
        android:id="@+id/favoriteImg"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:background="@drawable/round_star_rate_24"
        app:layout_constraintBottom_toBottomOf="@+id/favorite"
        app:layout_constraintEnd_toEndOf="@+id/favorite"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="@+id/favorite"
        app:layout_constraintTop_toTopOf="@+id/favorite"
        app:layout_constraintVertical_bias="0.533"
        android:onClick="onStartFavorites"/>

    <TextView
        android:id="@+id/chat"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/round"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.98"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.782" />

    <TextView
        android:id="@+id/chatImg"
        android:layout_width="55dp"
        android:layout_height="62dp"
        android:background="@drawable/round_chat_24"
        android:onClick="onStartChat"
        app:layout_constraintBottom_toBottomOf="@+id/chat"
        app:layout_constraintEnd_toEndOf="@+id/chat"
        app:layout_constraintHorizontal_bias="0.56"
        app:layout_constraintStart_toStartOf="@+id/chat"
        app:layout_constraintTop_toTopOf="@+id/chat"
        app:layout_constraintVertical_bias="0.444" />


</androidx.constraintlayout.widget.ConstraintLayout>