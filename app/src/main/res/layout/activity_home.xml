<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#5d515e"
    android:id="@+id/container"
    >


    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingBottom="8dp"
        tools:ignore="MissingConstraints">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:text="Send data" />

        <ToggleButton
            android:id="@+id/active"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right" />

    </LinearLayout>

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/lottie"
        android:layout_width="match_parent"
        android:layout_height="493dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:lottie_autoPlay="true"
        app:lottie_loop="true"
        app:lottie_rawRes="@raw/background" />

    <TextView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="250dp"
        android:background="@drawable/square"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Effects"
        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/favorite"
        app:layout_constraintTop_toBottomOf="@+id/effects"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_editor_absoluteX="127dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sounds"
        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/recordings"
        app:layout_constraintTop_toBottomOf="@+id/sounds"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_editor_absoluteX="224dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="21dp"
        android:layout_marginRight="21dp"
        android:text="Settings"
        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/logOut"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/settings"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sensor"
        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/profile"
        app:layout_constraintTop_toBottomOf="@+id/sensor"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_editor_absoluteX="27dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Favorites"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/favorite"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_editor_absoluteX="122dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="20dp"
        android:text="Log Out"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logOut"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Recordings"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/recordings"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_editor_absoluteX="214dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Profile"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profile"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_editor_absoluteX="27dp" />

    <TextView
        android:textColor="@color/white"
        android:id="@+id/sensors2osc_links"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:linksClickable="true"
        android:paddingTop="16dp"
        android:text="Effects"/>

    <TextView
        android:id="@+id/settings"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/round"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.98"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.156" />

    <TextView
        android:id="@+id/settingsImg"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:background="@drawable/round_settings_24"
        android:onClick="onStartLSettings"
        app:layout_constraintBottom_toBottomOf="@+id/settings"
        app:layout_constraintEnd_toEndOf="@+id/settings"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/settings"
        app:layout_constraintTop_toTopOf="@+id/settings"
        app:layout_constraintVertical_bias="0.533" />

    <TextView
        android:id="@+id/sensor"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/round"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintHorizontal_bias="0.025"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.156" />

    <TextView
        android:id="@+id/profile"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/round"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintHorizontal_bias="0.025"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.782" />

    <TextView
        android:id="@+id/profileImg"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:background="@drawable/round_perm_identity_24"
        app:layout_constraintBottom_toBottomOf="@+id/profile"
        app:layout_constraintEnd_toEndOf="@+id/profile"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="@+id/profile"
        app:layout_constraintTop_toTopOf="@+id/profile"
        app:layout_constraintVertical_bias="0.533" />



    <TextView
        android:id="@+id/sensorImg"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:background="@drawable/baseline_sensors_24"
        android:onClick="onStartSensors"
        app:layout_constraintBottom_toBottomOf="@+id/sensor"
        app:layout_constraintEnd_toEndOf="@+id/sensor"
        app:layout_constraintStart_toStartOf="@+id/sensor"
        app:layout_constraintTop_toTopOf="@+id/sensor" />

    <TextView
        android:id="@+id/recordings"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/round"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.661"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.782" />

    <TextView
        android:id="@+id/recordingsImg"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:background="@drawable/round_settings_voice_24"
        app:layout_constraintBottom_toBottomOf="@+id/recordings"
        app:layout_constraintEnd_toEndOf="@+id/recordings"
        app:layout_constraintStart_toStartOf="@+id/recordings"
        app:layout_constraintTop_toTopOf="@+id/recordings" />

    <TextView
        android:id="@+id/sounds"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/round"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.66"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.156" />


    <TextView
        android:id="@+id/soundsImg"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:background="@drawable/round_music_note_24"
        app:layout_constraintBottom_toBottomOf="@+id/sounds"
        app:layout_constraintEnd_toEndOf="@+id/sounds"
        app:layout_constraintStart_toStartOf="@+id/sounds"
        app:layout_constraintTop_toTopOf="@+id/sounds"
        tools:ignore="MissingConstraints"
        android:onClick="onStartSounds"/>

    <TextView
        android:id="@+id/effects"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:background="@drawable/round"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.318"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.156" />

    <TextView
        android:id="@+id/effectsImg"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/round_graphic_eq_24"
        app:layout_constraintBottom_toBottomOf="@+id/effects"
        app:layout_constraintEnd_toEndOf="@+id/effects"
        app:layout_constraintStart_toStartOf="@+id/effects"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        android:onClick="onStartEffects"/>

    <TextView
        android:id="@+id/favorite"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:background="@drawable/round"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.318"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.782" />

    <TextView
        android:id="@+id/favoriteImg"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:background="@drawable/round_star_rate_24"
        app:layout_constraintBottom_toBottomOf="@+id/favorite"
        app:layout_constraintEnd_toEndOf="@+id/favorite"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="@+id/favorite"
        app:layout_constraintTop_toTopOf="@+id/favorite"
        app:layout_constraintVertical_bias="0.533" />

    <TextView
        android:id="@+id/logOut"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/round"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.98"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.782" />

    <TextView
        android:id="@+id/logOutImg"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:background="@drawable/round_logout_24"
        app:layout_constraintBottom_toBottomOf="@+id/logOut"
        app:layout_constraintEnd_toEndOf="@+id/logOut"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toStartOf="@+id/logOut"
        app:layout_constraintTop_toTopOf="@+id/logOut"
        android:onClick="onStartLogOut"/>



</androidx.constraintlayout.widget.ConstraintLayout>